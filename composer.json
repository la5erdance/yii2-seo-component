{
	"name": "la5erdance/yii2-seo-component",
	"description": "Yii2 Component to manage SEO data and metadata",
	"keywords": [
		"yii2 seo", 
		"yii2 search engine optimization", 
		"yii2 metadata",
		"yii2 seo meta"
	],
	"homepage": "https://github.com/la5erdance/yii2-seo-component",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Andrey Tanasov",
			"email": "info@tanasov.net"
		}
	],
	"require": {
		"yiisoft/yii2": "*"
	},
	"autoload": {
		"psr-4": {
			"la5erdance\\seo\\": ""
		}
	}
	"repositories": [
		{
		    "type": "vcs",
		    "url": "https://github.com/la5erdance/yii2-seo-component"
		}
	],
	"config": {
		"process-timeout": 1800
	}		
	
}
